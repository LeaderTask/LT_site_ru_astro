{
	"name": "leader-task-site",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently \"npm run watch-images\" \"tinacms dev -c \\\"astro dev\\\"\"",
		"watch-images": "tsx tina/watch-media.ts",
		"prebuild": "npm run convert-images && npm run rewrite-content-webp",
		"build": "tinacms build && astro build",
		"build:astro": "astro build",
		"build:tina": "tinacms build",
		"preview": "astro preview",
		"astro": "astro",
		"astro:dev": "astro dev",
		"tina:dev": "tinacms dev",
		"tina:build": "tinacms build",
		"lint": "eslint . --ext .js,.ts,.mjs",
		"lint:fix": "eslint . --ext .js,.ts,.mjs --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "vue-tsc --noEmit && astro check",
		"convert-images": "tsx src/shared/scripts/convert-to-webp.ts",
		"rewrite-content-webp": "tsx src/shared/scripts/replace-content-webp.ts",
		"analyze-webp": "tsx src/shared/scripts/analyze-webp-sizes.ts"
	},
	"dependencies": {
		"@astrojs/mdx": "^4.3.6",
		"@astrojs/react": "^4.4.0",
		"@astrojs/rss": "^4.0.12",
		"@astrojs/sitemap": "^3.6.0",
		"@astrojs/vue": "^5.1.1",
		"@tailwindcss/vite": "^4.1.14",
		"@tanstack/react-virtual": "^3.13.12",
		"@tinacms/toolkit": "^1.7.13",
		"@vueuse/core": "^13.9.0",
		"astro": "^5.14.1",
		"better-sqlite3": "^11.10.0",
		"date-fns": "2.30.0",
		"dotenv": "^17.2.3",
		"marked": "^16.4.1",
		"swiper": "^12.0.2",
		"tailwindcss": "^4.1.14",
		"tinacms": "^2.9.0",
		"vue": "^3.5.22",
		"vue-eslint-parser": "^10.2.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.4",
		"@eslint/js": "^9.37.0",
		"@tailwindcss/typography": "^0.5.19",
		"@tinacms/cli": "^1.11.0",
		"@types/node": "^24.7.0",
		"@types/unist": "^3.0.3",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"astro-eslint-parser": "^1.2.2",
		"concurrently": "^9.1.2",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-markdown": "^5.1.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-vue": "^10.5.0",
		"hast": "^1.0.0",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"rehype": "^13.0.2",
		"sharp": "^0.34.4",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0",
		"vue-tsc": "^3.1.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"better-sqlite3",
			"esbuild"
		],
		"ignoredBuiltDependencies": []
	}
}
